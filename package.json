{
  "name": "ftc-maintenance-sripts",
  "repository": "https://github.com/technototes/PowerPlay2022.git",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "license": "CC0-1.0",
  "scripts": {
    "-----": "These are all for building 'stuff':",
    "fast": "yarn gradle :Twenty403:assembleDebug :Swerveteen750:assembleDebug",
    "fast16": "yarn gradle :Sixteen750:assembleDebug",
    "fast20": "yarn gradle :Twenty403:assembleDebug",
    "fastsw": "yarn gradle :Swerveteen750:assembleDebug",
    "build": "yarn gradle :Swerveteen750:build :Twenty403:build",
    "build16": "yarn gradle :Sixteen750:build",
    "build20": "yarn gradle :Twenty403:build",
    "buildsw": "yarn gradle :Swerveteen750:build",
    "full": "yarn gradle build",
    "gradle": "run-script-os",
    "gradle:win32": ".\\gradlew",
    "gradle:default": "./gradlew",
    "-___-": "This one is for moving everyone's branches forward as main moves:",
    "ffwd": "node scripts/fastforward.js",
    "-_-_-": "These scripts are for formatting code/scripts/markdown stuff:",
    "format": "prettier --write Sixteen750/sc/** Twenty403/src/** Swerveteen750/src/** MeepMeepTesting/src/** ForTeaching/src/** *.json scripts/*.js",
    "format16": "prettier --write Sixteen750/src/**",
    "format20": "prettier --write Twenty403/src/**",
    "formatsw": "prettier --write Swerveteen750/src/**",
    "formatjs": "prettier --write *.json scripts/*.js",
    "formatft": "prettier --write ForTeaching/src/**",
    "formatall": "yarn format && yarn formatjs",
    "--_--": "This will toggle using TechnoLib locally or from Maven",
    "libflip": "node scripts/flip.js lib",
    ".._..": "Disable/enable 'all' bots & subbuilds",
    "botflip": "node scripts/flip.js bot",
    "_____": "Stuff for connecting/disconnecting from the bot",
    "connect": "run-script-os",
    "connect:win32": "%LOCALAPPDATA%\\Android\\sdk\\platform-tools\\adb connect 192.168.43.1:5555",
    "connect:macos": "~/Library/Android/sdk/platform-tools/adb connect 192.168.43.1:5555",
    "connect:linux": "~/Android/Sdk/platform-tools/adb connect 192.168.43.1:5555",
    "disconnect": "run-script-os",
    "disconnect:win32": "%LOCALAPPDATA%\\Android\\sdk\\platform-tools\\adb disconnect 192.168.43.1:5555",
    "disconnect:macos": "~/Library/Android/sdk/platform-tools/adb disconnect 192.168.43.1:5555",
    "disconnect:linux": "~/Android/Sdk/platform-tools/adb disconnect 192.168.43.1:5555",
    "discall": "run-script-os",
    "discall:win32": "%LOCALAPPDATA%\\Android\\sdk\\platform-tools\\adb disconnect",
    "discall:macos": "~/Library/Android/sdk/platform-tools/adb disconnect",
    "discall:linux": "~/Android/Sdk/platform-tools/adb disconnect"
  },
  "devDependencies": {
    "prettier": "^2.8.3",
    "prettier-plugin-java": "^2.0.0",
    "run-script-os": "^1.1.6",
    "simple-git": "^3.16.0"
  }
}
